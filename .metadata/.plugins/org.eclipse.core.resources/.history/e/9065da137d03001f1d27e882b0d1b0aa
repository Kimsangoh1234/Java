package com.kh.variable.pre;

public class EnumPre {
	public enum Enume{
		빨강,
		주황,
		노랑,
		초록,
		파랑,
		남색,
		보라
	}
	public static void main(String[] args) {
	
		Enume[] 배열 = Enume.values();
		for(Enume 무 : 배열) {
			System.out.println(무);
		}
	}
//enum 무지개 {빨강~보라}
	
	
	//main
		//빨~보 출력
	
	//장바구니 = 배열에 담기
	//					enum 밑에있는 메서드
	
}
